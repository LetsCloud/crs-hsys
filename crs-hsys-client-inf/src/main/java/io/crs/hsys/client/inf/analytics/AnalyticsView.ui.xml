<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">

	<ui:style>
	.container {
		margin-left: auto;
		margin-right: auto;
		padding-left: 10px;
		padding-right: 10px;
	}
	</ui:style>

	<m:MaterialPanel ui:field="container" addStyleNames="{style.container}">
		<m:MaterialPanel ui:field="chartContainer" />
		<m:MaterialPanel ui:field="tableContainer" />
	</m:MaterialPanel>
</ui:UiBinder> 