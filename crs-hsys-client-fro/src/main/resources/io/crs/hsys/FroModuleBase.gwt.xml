<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
  "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to="fro_module">
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.inject.Inject' />
	<inherits name='com.google.gwt.editor.Editor' />
	<inherits name="com.google.gwt.resources.Resources" />

	<inherits name="com.google.gwt.i18n.CldrLocales" />
	<inherits name="com.google.gwt.i18n.I18N" />

	<!--GWTP -->
	<inherits name="com.gwtplatform.dispatch.rest.DispatchRest" />
	<inherits
		name="com.gwtplatform.dispatch.rest.delegates.ResourceDelegate" />
	<inherits name='com.gwtplatform.mvp.MvpWithEntryPoint' />
	<!-- <inherits name="com.gwtplatform.mvp.MvpWithFormFactor" /> -->

	<!-- GWT Material -->
	<inherits
		name="gwt.material.design.GwtMaterialBasicWithJQueryDebug" />
	<inherits name="gwt.material.design.GwtMaterialTableBasic" />
	<inherits
		name="gwt.material.design.addins.GwtMaterialAddinsBasic" />
	<inherits
		name="gwt.material.design.addext.GwtMaterialAddextBasic" />
	<inherits name="gwt.material.design.incubator.AddinsIncubator" />
	<!-- Font Awesome Icons import -->
    <inherits name="gwt.material.design.incubator.FontAwesomeResources" />

	<inherits name="com.google.common.collect.Collect" />

	<inherits name='com.allen_sauer.gwt.dnd.gwt-dnd' />
	<inherits name='com.bradrydzewski.gwt.calendar.Calendar' />
	<inherits
		name='com.bradrydzewski.gwt.calendar.theme.google.Google' />

	<source path="client" />
	<source path="shared" />

	<extend-configuration-property
		name="gin.ginjector.modules"
		value="io.crs.hsys.client.fro.gin.ClientModule" />

	<set-configuration-property
		name="gwtp.bootstrapper" value="io.crs.hsys.client.fro.FroApp" />

	<set-configuration-property
		name="CssResource.enableGss" value="true" />

	<public path="client/fro/resources"
		excludes="**/*.java,**/*.class" />

	<stylesheet src="css/fonts.css" />

	<add-linker name="xsiframe" />
</module>
