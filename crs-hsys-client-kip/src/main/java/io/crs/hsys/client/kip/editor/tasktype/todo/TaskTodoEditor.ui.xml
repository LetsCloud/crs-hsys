<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">

	<ui:with field="i18nCore"
		type="io.crs.hsys.client.core.i18n.CoreMessages" />

	<m:MaterialCollectionItem type="CHECKBOX"
		waves="DEFAULT">
		<m:MaterialLabel ui:field="label" />
		<m:MaterialCollectionSecondary ui:field="mcs">
			<m:MaterialCheckBox ui:field="checkBox" value="false" type="FILLED" />
		</m:MaterialCollectionSecondary>
	</m:MaterialCollectionItem>
</ui:UiBinder> 