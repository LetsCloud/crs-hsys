<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	/* Add CSS here. See the GWT docs on UI Binder for more details */
	</ui:style>

	<m:MaterialPanel>
		<m:MaterialRow>
			<m:MaterialColumn grid="s6">
				<m:MaterialTextBox ui:field="fromRoom"
					placeholder="Szobától" length="10" grid="s12" />
				<m:MaterialTextBox ui:field="fromFloor"
					placeholder="Emelettől" length="10" grid="s12" />
				<m:MaterialCheckBox ui:field="dirtyVacant" text="Piszkos" />
				<m:MaterialCheckBox ui:field="cleanVacant" text="Tiszta" />
				<m:MaterialCheckBox ui:field="okVacant" text="Rendben" />
				<m:MaterialCheckBox ui:field="outOfService" text="Out of service" />
				<m:MaterialCheckBox ui:field="showRoom" text="Show room" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s6">
				<m:MaterialTextBox ui:field="toRoom" placeholder="Szobáig"
					length="10" grid="s12" />
				<m:MaterialTextBox ui:field="toFloor" placeholder="Emeletig"
					length="10" grid="s12" />
				<m:MaterialCheckBox ui:field="dirtyOccupied"
					text="Érkezők" />
				<m:MaterialCheckBox ui:field="cleanOccupied"
					text="Megérkezett" />
				<m:MaterialCheckBox ui:field="okOccupied" text="Lakó" />
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialButton ui:field="filterButton" text="Primary"
			waves="LIGHT" backgroundColor="BLUE" textColor="WHITE" iconType="POLYMER"
			iconPosition="LEFT" />
	</m:MaterialPanel>
</ui:UiBinder> 